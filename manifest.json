{
    "name": "Walmart Order Automation",
    "description": "Generate a Walmart cart with the ingredients needed for the recipes specified",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }

}
